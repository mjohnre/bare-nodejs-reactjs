  "dependencies": {
    "@babel/cli": "~7.6.2",
    "@babel/core": "~7.6.2",
    "@babel/node": "~7.6.2",
    "@babel/preset-env": "~7.6.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "npm-run-all": "~4.1.5",
    "rimraf": "~3.0.0"
  },
  "devDependencies": {
    "nodemon": "~1.19.2"
  }
}

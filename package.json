{
  "name": "notebook01",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server:dev": "babel-node -r node_modules/dotenv/config src/bin/www.js",
    "server:prod": "node ./dist/bin/www",
    "dev": "npm-run-all server:dev",
    "watch": "nodemon server:dev",
    "test": "jest --setupFiles dotenv/config",
    "test:coverage": "jest --setupFiles dotenv/config --coverage",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "npm-run-all clean build server:prod"
  },
  "dependencies": {
    "@babel/cli": "~7.6.2",
    "@babel/core": "~7.6.2",
    "@babel/node": "~7.6.2",
    "@babel/preset-env": "~7.6.2",
    "cookie-parser": "~1.4.4",
    "express": "~4.17.3",
    "helmet": "~3.21.1",
    "morgan": "~1.9.1",
    "npm-run-all": "~4.1.5",
    "rimraf": "~3.0.0",
    "winston": "~3.2.1"
  },
  "devDependencies": {
    "@types/jest": "~24.0.18",
    "babel-jest": "~24.9.0",
    "dotenv": "~8.1.0",
    "jest": "~24.9.0",
    "nodemon": "~1.19.2",
    "supertest": "~4.0.2"
  }
}
